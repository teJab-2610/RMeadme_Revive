{
  "manifest_version": 3,
  "name": "GitHub Button Extension",
  "version": "1.0",
  "description": "Adds a button when entering GitHub.",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "js": ["content.js", "showdown.js"]
    }
  ]
}
